{
  "openapi": "3.0.0",
  "info": {
    "title": "AGRI-MODAL APPLICATION API",
    "description": "API Documentation for AGRI-MODAL",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/api/v1"
    }
  ],
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      },
      "RefreshToken": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "paths": {
    "/auth/login": { "$ref": "./auth/login.json" },
    "/auth/client/login": { "$ref": "./auth/loginClient.json" },
    "/auth/signup" : { "$ref": "./auth/signup.json" },
    "/auth/refresh-token": { "$ref": "./auth/refresh.json" },

    
    "/communities": { "$ref": "./community/community.json" },
    "/communities/user-community" : { "$ref": "./community/userCommunity.json" },
    "/communities/user-community/{communityId}" : { "$ref": "./community/userCommunity.json" }
   

  }
}